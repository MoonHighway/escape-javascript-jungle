{
  "name": "weather-bug",
  "version": "0.0.0",
  "description": "A full stack current weather application",
  "type": "module",
  "main": "./src/index.js",
  "scripts": {
    "build:dev": "webpack --watch",
    "build": "webpack --mode production",
    "prestart": "npm run build",
    "start": "node ./src/server.js --experimental-modules",
    "start:dev": "nodemon ./src/server.js --experimental-modules",
    "development": "concurrently \"npm run build:dev\" \"npm run start:dev\""
  },
  "keywords": [
    "Weather",
    "10",
    "day",
    "forcast"
  ],
  "author": "Alex Banks",
  "license": "MIT",
  "dependencies": {
    "chart.js": "^2.9.4",
    "express": "^4.17.1",
    "weather-js": "^2.0.0"
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.7",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0"
  }
}
